package quiz02;

public class MultiplicationTable {

	// property
	private int startDan;
	private int endDan;
	private int startNum;
	private int endNum;
	private Calculator calculator;
	
	// constructor
	
	// method
	public void gugudan() {
		int[][] dan = new int[3][3];
		startDan = 3;
		endDan = 3;
		startNum = 1;
		endNum = 3;
		
		// 구구단 초기화
		for(int i=0; i<dan.length; i++) {
			for(int j=0; j<dan[i].length; j++) {
				dan[i][j]=(i+)*(j+1); // i -> 2단부터 시작, j -> x1
				System.out.print(dan[i][j]+"\t");
			}
			System.out.println();
		}

	}
	
	
}


